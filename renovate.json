{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:earlyMondays",
    ":prHourlyLimit4",
    ":rebaseStalePrs",
    ":semanticCommits"
  ],
  "labels": ["dependencies", "python"],
  "semanticCommitScope": "{{ depType }}",
  "rangeStrategy": "bump",
  "constraints": {
    "python": "==3.11"
  }
}
